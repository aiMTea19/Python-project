    "    \"\"\"\n",
    "    while b != 0 :\n",
    "        a *= 2\n",
    "        b -= 1\n",
    "    if a % c == 0 :\n",
    "        return f\"True ({a} is divisible by {c})\"\n",
    "    else :\n",
    "        return f\"False ({a} isn't divisible by {c})\""
